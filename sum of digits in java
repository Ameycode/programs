import java.util.*;
class SOD 
{
	public static void main(String arg[])
	{
		int no,digit,sum=0;
		Scanner sc=new Scanner(System.in);
		System.out.print("\n enter number= ");
		no=sc.nextInt();
		while(no>0)
		{
			digit=no%10;
			sum=sum+digit;
			no=no/10;
		}
		System.out.print("\n Sum of Digits= "+sum);
	}
}
