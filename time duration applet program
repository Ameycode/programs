import java.applet.*;
import java.awt.*;
public class myapplet extends Applet
{
public void init()
{
System.out.println("initialised...");
}
public void paint(Graphics g)
{
for(int i=0;i<=360;i++)
{
g.drawOval(100,100,200,200);
try
{
Thread.sleep(100);
}
catch(Exception e){}
}
}
}
//<applet code="myappet" width=500 height=500></applet>
